<Program>
	<FuncDef type="bool">
		<Identifier>f</Identifier>
		<Parameter type="int">x</Parameter>
		<Block>
			<If>
				<Condition>
					<BinExprNode Op=">">
						<Identifier>x</Identifier>
						<IntConst>2</IntConst>
					</BinExprNode>
				</Condition>
				<IfBlock>
					<Block>
						<Return>
							<BoolConst>true</BoolConst>
						</Return>
					</Block>
				</IfBlock>
				<ElseBlock>
					<Block>
						<If>
							<Condition>
								<BinExprNode Op="<">
									<Identifier>x</Identifier>
									<IntConst>0</IntConst>
								</BinExprNode>
							</Condition>
							<IfBlock>
								<Block>
									<Return>
										<BoolConst>false</BoolConst>
									</Return>
								</Block>
							</IfBlock>
							<ElseBlock>
								<Block>
									<Print>
										<StringConst>I don't return here </StringConst>
									</Print>
									<Return>
										<BoolConst>false</BoolConst>
									</Return>
								</Block>
							</ElseBlock>
						</If>
					</Block>
				</ElseBlock>
			</If>
		</Block>
	</FuncDef>
	<Print>
		<FuncCall>
			<Identifier>f</Identifier>
			<Arg>
				<IntConst>1</IntConst>
			</Arg>
		</FuncCall>
	</Print>
</Program>
