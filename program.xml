<Program>
	<FuncDef type="int">
		<Identifier>fib</Identifier>
		<Parameter type="int">n</Parameter>
		<Block>
			<If>
				<Condition>
					<BinExprNode Op="==">
						<Identifier>n</Identifier>
						<IntConst>0</IntConst>
					</BinExprNode>
				</Condition>
				<IfBlock>
					<Block>
						<Return>
							<IntConst>0</IntConst>
						</Return>
					</Block>
				</IfBlock>
				<ElseBlock>
					<Block>
						<If>
							<Condition>
								<BinExprNode Op="==">
									<Identifier>n</Identifier>
									<IntConst>1</IntConst>
								</BinExprNode>
							</Condition>
							<IfBlock>
								<Block>
									<Return>
										<IntConst>1</IntConst>
									</Return>
								</Block>
							</IfBlock>
							<ElseBlock>
								<Block>
									<Return>
										<BinExprNode Op="+">
											<FuncCall>
												<Identifier>fib</Identifier>
												<Arg>
													<BinExprNode Op="-">
														<Identifier>n</Identifier>
														<IntConst>1</IntConst>
													</BinExprNode>
												</Arg>
											</FuncCall>
											<FuncCall>
												<Identifier>fib</Identifier>
												<Arg>
													<BinExprNode Op="+">
														<Identifier>n</Identifier>
														<IntConst>2</IntConst>
													</BinExprNode>
												</Arg>
											</FuncCall>
										</BinExprNode>
									</Return>
								</Block>
							</ElseBlock>
						</If>
					</Block>
				</ElseBlock>
			</If>
		</Block>
	</FuncDef>
	<Print>
		<FuncCall>
			<Identifier>fib</Identifier>
			<Arg>
				<IntConst>6</IntConst>
			</Arg>
		</FuncCall>
	</Print>
</Program>
